<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ngemobi.mapper.LoginMapper">


<select id="selectUser" parameterType="map" resultType="com.ngemobi.bean.User">
	select username,password from regist_login where username=#{username}and password=#{password} 
</select>

</mapper>